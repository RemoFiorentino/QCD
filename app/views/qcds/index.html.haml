        %td= qcd.grupo
        %td= link_to 'Show', qcd
        %td= link_to 'Edit', edit_qcd_path(qcd)
        %td= link_to 'Destroy', qcd, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

